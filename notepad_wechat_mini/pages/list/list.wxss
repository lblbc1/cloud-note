
.page-wrapper {
    margin: 0;
    font-size: 28rpx;
    line-height: 1.5;
    color: #333;
    background-color: #efefef;
    overflow-x: hidden;
    -webkit-overflow-scrolling: touch;
    -webkit-tap-highlight-color: transparent;
}

.blog-item-row{
  display: flex;
  height: 100rpx;
  margin: 10rpx 0;
  background-color: #fff;
}
.blog-index{
  line-height: 100rpx;
  padding-left: 10rpx;
}
.blog-title{
  margin-left: 10rpx;
  font-size: 40rpx;
  line-height: 100rpx;
  color: #333;
}
.blog-btn{
  right: 0;
  position: absolute;
  line-height: 80rpx;
  background-color: #337AFF;
  color: #fff;
  padding: 0 20rpx;
  /* margin-right: 20rpx; */
  margin: 10rpx 20rpx 10rpx 0 ;
}
.float-img{
  position: fixed;
  bottom: 50rpx;
  right: 50rpx;
  height: 100rpx;
  width: 100rpx;
}